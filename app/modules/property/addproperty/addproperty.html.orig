<div class="row">
	<div class="col-md-13">
		<div class="main-content">		
			<ng-include src="'modules/component/toolbar.html'"></ng-include>						
			<h3 class="text-center" title="Add new Property">Add New Property</h3>
			<form class="form-horizontal myForm " name="myForm" novalidate>	
			
				<tabset>
				<p class="paraMand">* Fields are mandatory</p>
					<tab heading="Property" title="Property information">
					<h3>Property Information:</h3>
						<div class="form-group">
							
							<label for="profor" class="col-md-2  control-label">* Property for</label>
							<div class="col-md-9"  ng-model="property.property_for">
								<label class="radio-inline">
									<input type="radio" name="property_for"  id="Sale" value="Sale" checked="true">Sale
								</label>
								<label class="radio-inline">
								  <input type="radio" name="property_for"  id="Rent" value="Rent"> Rent
								</label>
								<label class="radio-inline">
								  <input type="radio" name="property_for"  id="PG" value="PG"> PG
								</label>												
							</div>	
						</div>
						<div class="form-group"> 
							<label for="Featured" class="col-md-2 control-label">* &nbsp Featured</label> 
								<div class="col-md-9 "  ng-model="property.featured">
									<label class="radio-inline">
									  <input type="radio" name="featured" id="Yes" value="yes" checked="true"> Yes
									</label>
									<label class="radio-inline">
									  <input type="radio" name="featured" id="No" value="No" ng-model="property.featured "> No
									</label>								
							</div>	
						</div>
						<div class="form-group">
							<label for="Category" class="col-md-2 control-label">* Property Category </label>
							<div class="col-md-9">
								<select  id="Category"  name="proCate" ng-model="property.category" class="form-control" required>
									<option selected value="">Select Category</option>
									<option value="Residential">Residential Property</option>
									<option value="Commercial">Commercial Property</option>
									<option value="Agricultural">Agricultural Land</option>
									<option value="Industrial">Industrial Property</option>			
								</select>
								<span style="color:red" ng-show="myForm.proCate.$dirty && myForm.proCate.$invalid">
									<span ng-show="myForm.proCate.$error.required">Property selection is required .</span>
								</span>				 
							</div>
						</div>
						<div class="form-group">
							<label for="type" class="col-md-2  control-label">* Property Type </label>
							<div class="col-sm-9">
								<input type="text"  id="type" class="form-control" name="proType" ng-model="property.type" required>
								<span style="color:red" ng-show="myForm.proType.$dirty && myForm.proType.$invalid">
									<span ng-show="myForm.proType.$error.required">Property type  is required .</span>
								</span>
							</div>
						</div>	
						<div class="form-group">
							<label for="PropPrice" class="col-md-2 control-label ">Property Price: </label>
							<div class="col-md-6">
								<input type="text" id="PropPrice" class="form-control" name="propertyPrice" ng-model="property.propertyPrice" ng-pattern="/^\d{0,9}[0-9]$/" required>
								<p ng-show="myForm.propertyPrice.$error.pattern && (myForm.propertyPrice.$dirty || submitted)" class="help-block">Enter only no.</p>
							</div>
							<div class="col-md-3">
								<select class="form-control" ng-model="property.currency" required>
									<option value="" selected>In Rupees</option>
									<option value="Thousand">Thousand</option>
									<option value="Lac">Lac</option>
									<option value="Carore">Carore</option>					
								</select>
								<span style="color:red" ng-show="myForm.propertyPrice.$dirty && myForm.propertyPrice.$invalid">
									<span ng-show="myForm.propertyPrice.$error.required">Property Price is required .</span>
								</span>				 
							</div>				
						</div>	
					</tab>
					<tab heading="Area Details" title="Property Area Details">
						<h3>Property Area Details</h3>
						<div class="form-group">
							<label for="BuiltArea" class="col-md-2 control-label">* Built Up Area </label>
							<div class="col-md-6">
								<input type="text"  id="BuiltArea" class="form-control" name="builtArea" ng-model="property.buildup_area" ng-pattern="/^\d{0,9}[0-9]$/" required>
								<p ng-show="myForm.builtArea.$error.pattern && (myForm.builtArea.$dirty || submitted)" class="help-block">Enter only no.</p>
							</div>
							<div class="col-md-3">
								<select class="form-control" ng-model="property.build_unit"  title="" required >
									<option selected value="">Unit Area</option>
									<option value="Feet">Sq. Feet</option>
									<option value="Cent">Cent</option>
									<option value="Yard">Sq. Yard</option>
													
								</select>
								<span style="color:red" ng-show="myForm.builtArea.$dirty && myForm.builtArea.$invalid">
								  <span ng-show="myForm.builtArea.$error.required">Built Area is required .</span>
								</span>				 
							</div>
						</div>		  
						<div class="form-group">
							<label for="PlotArea" class="col-md-2 control-label ">* Plot/ Land Area</label>
							<div class="col-md-6">
								<input type="text" id="PlotArea" class="form-control" name="plotArea" ng-model="property.land_area"  ng-pattern="/^\d{0,9}[0-9]$/" required>
								<p ng-show="myForm.plotArea.$error.pattern && (myForm.plotArea.$dirty || submitted)" class="help-block">Enter only no.</p>
							</div>
							<div class="col-md-3">
								<select class="form-control" ng-model="property.land_unit" required>
									<option selected value="">Unit Area</option>
									<option value="Feet">Sq. Feet</option>
									<option value="Cent">Cent</option>
									<option value="Yard">Sq. Yard</option>					
								</select>
								<span style="color:red" ng-show="myForm.plotArea.$dirty && myForm.areaUnit.$invalid">
								  <span ng-show="myForm.areaUnit.$error.required">Built Area is required .</span>
								</span>				 
							</div>				
						</div>		  
					<div class="form-group">
						<label for="CarpetArea" class="col-md-2 control-label">Carpet Area </label>
						<div class="col-md-6">
							<input type="text" id="CarpetArea" class="form-control" name="carpetArea" ng-model="property.carpet_area" ng-pattern="/^\d{0,9}[0-9]$/" required>
							<p ng-show="myForm.carpetArea.$error.pattern && (myForm.carpetArea.$dirty || submitted)" class="help-block">Enter only no.</p>				 
						</div>
						<div class="col-md-3">
							<select class="form-control" ng-model="property.carpet_unit" required>
								<option selected value="">Unit Area</option>
									<option value="Feet">Sq. Feet</option>
									<option value="Cent">Cent</option>
									<option value="Yard">Sq. Yard</option>					
							</select>
							<span style="color:red" ng-show="myForm.carpetArea.$dirty && myForm.carpetArea.$invalid">
							  <span ng-show="myForm.carpetArea.$error.required">Carpet Area is required .</span>
							</span>				 
						</div>				
					</div>	  
				</tab>
